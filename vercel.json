{
    "version": 2,
    "build" : [
        {
            "src":"djanrest.wsgi.py",
            "use": "@vercel/python", 
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "djanrest/wsgi.py"
        }
    ]

}